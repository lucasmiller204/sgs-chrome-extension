{
    "name" : "Smart Gen Society Self Reporter",
    "description" : "self-report form made to work quickly with webpages to help federal law enforcement receive actionable data to prevent child grooming",
    "version" : "1.0",
    "manifest_version" : 3,
    "background" : {
        "service_worker" : "background.js"
    },
    "permissions" : ["storage", "activeTab", "scripting"],
    "action" : {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/sgs-badge.png",
            "32": "/images/sgs-badge.png",
            "48": "/images/sgs-badge.png",
            "128": "/images/sgs-badge.png"
        }
    },
    "icons": {
        "16": "/images/sgs-badge.png",
        "32": "/images/sgs-badge.png",
        "48": "/images/sgs-badge.png",
        "128": "/images/sgs-badge.png"
    },
    "options_page": "options.html"
}