{
    "name": "Smart Gen Society Self Reporter",
    "description": "self-report form made to work quickly with webpages to help federal law enforcement receive actionable data to prevent child grooming",
    "version": "1.0",
    "manifest_version": 4,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["notifications", "fontSettings", "storage", "activeTab", "scripting", "tabs"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/sgs-badge16.png",
            "32": "/images/sgs-badge32.png",
            "48": "/images/sgs-badge48.png",
            "128": "/images/sgs-badge128.png"
        }
    },
    "icons": {
        "16": "/images/sgs-badge16.png",
        "32": "/images/sgs-badge32.png",
        "48": "/images/sgs-badge48.png",
        "128": "/images/sgs-badge128.png"
    },
    "options_page": "options.html"
}